<?xml version="1.0" encoding="UTF-8"?>
<ivy-module version="2.0" xmlns:m="http://ant.apache.org/ivy/maven">
  <info organisation="${ivy.artifact.group}" module="${ivy.artifact.id}" revision="${project.revision}" />

  <configurations>
    <conf name="default" />
    <conf name="test" visibility="private" />
  </configurations>

  <publications>
    <artifact name="${ivy.artifact.id}" type="jar" conf="default" />
  </publications>

  <dependencies>
    <dependency conf="test->default" org="junit" name="junit" rev="4.5"/>
    <dependency conf="test->default" org="org.apache.hadoop.hive" name="hive-jdbc" rev="${dependency.hive-jdbc.revision}"/>
    <dependency conf="test->default" org="pentaho-kettle" name="kettle-engine" rev="${dependency.kettle.revision}"/>
    <dependency conf="test->default" org="pentaho-kettle" name="kettle-core" rev="${dependency.kettle.revision}"/>
    <dependency conf="test->default" org="pentaho-kettle" name="kettle-db" rev="${dependency.kettle.revision}"/>
    <dependency conf="test->default" org="${ivy.artifact.group}" name="pentaho-hadoop-shims-api" rev="${project.revision}" />
    
  </dependencies>
</ivy-module>
